{
  "name": "text analyzer service",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "typeorm": "ts-node -r dotenv/config node_modules/typeorm/cli.js",
    "test": "jest src/Test",
    "test:unit": "jest src/Test/Unit",
    "test:e2e": "jest src/Test/E2E",
    "migrate": "ts-node -r dotenv/config node_modules/typeorm/cli.js migration:run -d migration.ts",
